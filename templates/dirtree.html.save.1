
<!doctype html>
<title>Path: {{ tree.name }}</title>
<h1>{{ tree.name }}</h1>
<ul>
{%- for item in tree.children recursive %}
/Users/at/Downloads     <li>{{ item.name }}
    {%- if item.children -%}
	<!--<ul>{{loop (item.children)}}</ul>-->
	{%- for items in item.children %}
		<a download href="{{url_for('download_file',path='tree.name/item.name/items.name')}}"><ul>{{items.name}}</ul></a>
		
	{%- endfor %}
    {%- endif %}</li>
{%- endfor %}
</ul>
